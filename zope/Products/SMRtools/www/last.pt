<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    lang="en"
    metal:use-macro="here/main_template/macros/master"
    i18n:domain="plone">
    <body>
        <div metal:fill-slot="main">
            <table>
                <caption>Processed files during the last 24 hours</caption>
                <thead>
                    <tr>
                        <th>date</th>
                        <th>measured date</th>
                        <th>orbit</th>
                        <th>fm</th>
                        <th>fqid</th>
                        <th>ratio</th>
                    </tr>
                </thead>
                <tbody tal:repeat="statusline python:context.smrstatus.lastList()">
                    <tr>
                        <td tal:content="python:statusline[0]">20071009 08:22</td>
                        <td tal:content="python:statusline[1]">19761009 08:22</td>
                        <td tal:content="python:hex(statusline[2])">8C54</td>
                        <td tal:content="python:statusline[3]">1</td>
                        <td tal:content="python:statusline[4]">29</td>
                        <td tal:content="python:statusline[8]">.99</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </body>
</html>
