<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    lang="en"
    metal:use-macro="here/main_template/macros/master"
    i18n:domain="plone">
    <body>
        <div metal:fill-slot="main" tal:define="datetime nocall:here/browse_quick/DateTime">

            <table tal:define="weeks python:here.browse_quick.getWeeksList(datetime.year(),datetimd.month())" >

                <thead tal:define="daylist python:here.browse_quick.getDays()">
                    <tr >
                        <td tal:repeat="name daylist" tal:content="name"> Sunday </td>
                    </tr>
                </thead>

                <tbody>
                    <tr tal:repeat="week weeks">
                        <td tal:repeat="day week" tal:content="day"> 24 </td>
                    </tr>
                </tbody>

            </table>

        </div>
    </body>
</html>
