<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    lang="en"
    metal:use-macro="here/main_template/macros/master"
    i18n:domain="plone">
    <body>
        <div metal:fill-slot="main">
            <table>
                <caption>Jobs in the queue</caption>
                <thead>
                    <tr>
                        <th>queue</th>
                        <th>id</th>
                        <th>wall</th>
                        <th>used</th>
                        <th>node</th>
                    </tr>
                </thead>
                <tbody tal:repeat="statusline python:context.smrstatus.currentList()">
                    <tr>
                        <td tal:content="python:statusline[2]">rerun</td>
                        <td tal:content="python:statusline[3]">id3</td>
                        <td tal:content="python:statusline[8]">04:00</td>
                        <td tal:content="python:statusline[10]">0:23</td>
                        <td tal:content="python:statusline[11]">bixbite/0</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </body>
</html>
